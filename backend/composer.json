{
    "name": "marekskopal/fingather",
    "type": "project",
    "description": "FinGather",
    "homepage": "https://www.fingather.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Skopal",
            "homepage": "https://www.marekskopal.cz"
        }
    ],
    "require": {
        "php": ">=8.3",
        "league/route": "dev-php8",
        "tracy/tracy": "^2.10",
        "thecodingmachine/safe": "^2.5",
        "psr/log": "^3.0",
        "spiral/roadrunner-http": "^3.3",
        "nyholm/psr7": "^1.8",
        "laminas/laminas-diactoros": "^3.3",
        "league/container": "^4.2",
        "firebase/php-jwt": "^6.10",
        "php-http/discovery": "^1.19",
        "cycle/orm": "^2.5",
        "cycle/annotated": "^4.1",
        "symfony/console": "^7.0",
        "cycle/migrations": "^4.2",
        "cycle/schema-migrations-generator": "^2.2",
        "league/csv": "^9.12",
        "php-decimal/php-decimal": "^1.1",
        "spiral/roadrunner-jobs": "^4.3",
        "symfony/mailer": "^7.0",
        "cycle/entity-behavior-uuid": "^1.1",
        "marekskopal/twelvedata": "^0.2",
        "guzzlehttp/guzzle": "^7.8",
        "phpoffice/phpspreadsheet": "^2.0",
        "spiral/roadrunner-kv": "^4.0",
        "marekskopal/cycle-decimal": "^0.3",
        "marekskopal/buggregator-client": "^0.1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "phpunit/phpunit": "^11.0",
        "spatie/ray": "^1.39",
        "thecodingmachine/phpstan-safe-rule": "^1.2",
        "slevomat/coding-standard": "^8.14",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "FinGather\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FinGather\\Tests\\": [
                "tests/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/marekskopal/route"
        }
    ]
}
