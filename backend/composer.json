{
    "name": "marekskopal/fingather",
    "type": "project",
    "description": "FinGather",
    "homepage": "https://www.fingather.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Marek Skopal",
            "homepage": "https://www.marekskopal.cz"
        }
    ],
    "require": {
        "php": ">=8.5",
        "ext-decimal": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "contributte/redis": "^0.6.0",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/psr7": "^2.7",
        "laminas/laminas-diactoros": "^3.3",
        "league/container": "^5.0",
        "league/csv": "^9.12",
        "league/route": "^6.1",
        "marekskopal/buggregator-client": "^0.1.0",
        "marekskopal/openfigi": "^0.2",
        "marekskopal/orm": "^0.9",
        "marekskopal/orm-decimal": "^1.0",
        "marekskopal/orm-migrations": "^0.9",
        "marekskopal/router": "^1.1",
        "marekskopal/trading212": "^1.0",
        "marekskopal/twelvedata": "^0.9.0",
        "nette/caching": "^3.3",
        "nette/utils": "^4.0",
        "nyholm/psr7": "^1.8",
        "php-decimal/php-decimal": "^1.1",
        "php-http/discovery": "^1.19",
        "phpoffice/phpspreadsheet": "^5.0",
        "predis/predis": "^2.3",
        "psr/container": "^2.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "ramsey/uuid": "^4.7",
        "spiral/goridge": "^4.2",
        "spiral/roadrunner": "^2025.1",
        "spiral/roadrunner-http": "^3.3",
        "spiral/roadrunner-jobs": "^4.3",
        "spiral/roadrunner-worker": "^3.5",
        "symfony/console": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mime": "^7.0",
        "tracy/tracy": "^2.10"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.0",
        "shipmonk/composer-dependency-analyser": "^1.5",
        "shipmonk/phpstan-rules": "^4.0",
        "slevomat/coding-standard": "^8.14",
        "spatie/ray": "^1.39",
        "tomasvotruba/cognitive-complexity": "^1.0",
        "tomasvotruba/unused-public": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "FinGather\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FinGather\\Tests\\": [
                "tests/"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "8.5"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
