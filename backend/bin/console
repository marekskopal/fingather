#!/usr/bin/env php
<?php

declare(strict_types=1);

require __DIR__ . '/../vendor/autoload.php';

use FinGather\Command\ApiImportPrepareCheckCommand;
use FinGather\Command\ApiImportProcessCheckCommand;
use FinGather\Command\BenchmarkPortfolioValueCommand;
use FinGather\Command\CacheClearCommand;
use FinGather\Command\DividendCalendarWarmupCommand;
use FinGather\Command\EmailPortfolioSummaryCommand;
use FinGather\Command\ExchangeRateUpdateCommand;
use FinGather\Command\GoalCheckCommand;
use FinGather\Command\MigrationGenerateCommand;
use FinGather\Command\MigrationRunCommand;
use FinGather\Command\PriceAlertCheckCommand;
use FinGather\Command\SplitUpdateCommand;
use FinGather\Command\TickerDataUpdateCommand;
use FinGather\Command\TickerFundamentalUpdateCommand;
use FinGather\Command\TickerUpdateCommand;
use FinGather\Command\TickerLogoUpdateCommand;
use FinGather\Command\WarmupAllCommand;
use FinGather\Command\WarmupAsyncCommand;
use FinGather\Command\WarmupUserCommand;
use Symfony\Component\Console\Application;

$application = new Application();

$application->addCommand(new ApiImportPrepareCheckCommand());
$application->addCommand(new ApiImportProcessCheckCommand());
$application->addCommand(new BenchmarkPortfolioValueCommand());
$application->addCommand(new CacheClearCommand());
$application->addCommand(new DividendCalendarWarmupCommand());
$application->addCommand(new EmailPortfolioSummaryCommand());
$application->addCommand(new ExchangeRateUpdateCommand());
$application->addCommand(new GoalCheckCommand());
$application->addCommand(new MigrationGenerateCommand());
$application->addCommand(new MigrationRunCommand());
$application->addCommand(new PriceAlertCheckCommand());
$application->addCommand(new SplitUpdateCommand());
$application->addCommand(new TickerDataUpdateCommand());
$application->addCommand(new TickerFundamentalUpdateCommand());
$application->addCommand(new TickerLogoUpdateCommand());
$application->addCommand(new TickerUpdateCommand());
$application->addCommand(new WarmupAllCommand());
$application->addCommand(new WarmupAsyncCommand());
$application->addCommand(new WarmupUserCommand());

$application->run();